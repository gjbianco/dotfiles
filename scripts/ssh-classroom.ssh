# classroom config ------------
Host class*
  StrictHostKeyChecking no
  IdentityFile ~/.ssh/classroom-keys/instructor_key

Host classroom
  User instructor
  # HostName <FIP>

Host class-bastion
  User student
  HostName bastion
  ProxyJump classroom

Host class-workstation
  User student
  HostName workstation
  ProxyJump class-bastion

